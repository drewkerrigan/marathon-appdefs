{
  "id": "/weblogic-medrec",
  "cmd": "printenv && cd $DOMAIN_HOME && ./startSample.sh",
  "cpus": 1,
  "mem": 4098,
  "disk": 0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "tkrausjr/1221-medrec",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 7001,
          "hostPort": 0,
          "servicePort": 10002,
          "protocol": "tcp",
          "labels": {}
        },
        {
          "containerPort": 5556,
          "hostPort": 0,
          "servicePort": 10003,
          "protocol": "tcp",
          "labels": {}
        },
        {
          "containerPort": 8001,
          "hostPort": 0,
          "servicePort": 10004,
          "protocol": "tcp",
          "labels": {}
        }
      ],
      "privileged": false,
      "parameters": [],
      "forcePullImage": false
    }
  },
  "healthChecks": [
    {
      "path": "/console",
      "protocol": "HTTP",
      "portIndex": 0,
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3,
      "ignoreHttp1xx": false
    }
  ],
  "labels": {
    "HAPROXY_GROUP": "external"
  },
  "portDefinitions": [
    {
      "port": 10001,
      "protocol": "tcp",
      "labels": {}
    },
    {
      "port": 10002,
      "protocol": "tcp",
      "labels": {}
    },
    {
      "port": 10003,
      "protocol": "tcp",
      "labels": {}
    }
  ]
}