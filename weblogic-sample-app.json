{
  "id": "/weblogic-app",
  "cmd": "printenv && pwd && wlst /u01/oracle/app-deploy.py && startWebLogic.sh",
  "cpus": 1,
  "mem": 4098,
  "disk": 0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "tkrausjr/1221-appdeploy:v2",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 8001,
          "hostPort": 0,
          "servicePort": 10004,
          "protocol": "tcp",
          "name": "weblogic-management-server",
          "labels": {}
        },
        {
          "containerPort": 5556,
          "hostPort": 0,
          "servicePort": 10005,
          "protocol": "tcp",
          "name": "weblogic-administration-server",
          "labels": {}
        },
        {
          "containerPort": 7001,
          "hostPort": 0,
          "servicePort": 10006,
          "protocol": "tcp",
          "name": "weblogic-node-manager",
          "labels": {}
        }
      ],
      "privileged": false,
      "parameters": [],
      "forcePullImage": false
    }
  },
  "healthChecks": [
    {
      "path": "/console",
      "protocol": "HTTP",
      "portIndex": 0,
      "gracePeriodSeconds": 300,
      "intervalSeconds": 25,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3,
      "ignoreHttp1xx": false
    }
  ],
  "labels": {
    "HAPROXY_GROUP": "external"
  }
}